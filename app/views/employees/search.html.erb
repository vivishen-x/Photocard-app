<% provide(:title, "Search Result") %>
<div class="container-fluid">
	<h1><%= pluralize(@employees.count, 'Employee') %></h1>
  <h3>Searching for "<%= params[:search] %>"</h3>
  <%= will_paginate %>
	<div class="row">
		<div class="col-md-3">
		</div>


		<div class="col-md-6">

        <%= render @employees %>
		</div>


		<div class="col-md-3">
		</div>
	</div>
  <%= will_paginate %>
</div>
